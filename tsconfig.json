{
  "compilerOptions": {
    "useUnknownInCatchVariables": false,
    "target": "es2015",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "paths": {
      "react": [
        "./node_modules/@types/react"
      ]
    },
    "incremental": true,
    "baseUrl": ".",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.jsx",
    "**/*.js",
    "**/*.tsx",
    "src/pages/api/products/index.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
